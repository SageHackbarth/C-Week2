@model SageHackbarthWeek2.Models.GenderModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
	<div>
		<div>
			@Html.DropDownListFor(m => m.Personel, new SelectList(Model.Personel, "FName", "FName"), "Select Person")
		</div>
		<div>
			@Html.LabelFor(m => m.PersonelGender)
			@Html.TextBoxFor(m => m.PersonelGender)
		</div>
		<div>
			@Html.LabelFor(m => m.Age)
			@Html.DisplayFor(m => m.Age)
		</div>
		<div>
			@Html.LabelFor(m => m.Major)
			@Html.DisplayFor(m => m.Major)
		</div>
	</div>
</body>
</html>
